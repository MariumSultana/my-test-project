
public class New1 {
 public static void main(String[] args) {

	 int i[][]=new int[3][3];
	 i[0][0]=2;
	 i[0][1]=3;
	 i[0][2]=4;
	 
	 i[1][0]=2;
	 i[1][1]=3;
	 i[1][2]=4;
	 
	 i[2][0]=2;
	 i[2][1]=3;
	 i[2][2]=4;
	 int rows = i.length;
	 int cols = i[0].length;
	 
	 for(int rowNum=0; rowNum<rows; rowNum++){
		 for(int colNum=0;colNum<cols; colNum++ )
		 System.out.println(i[rowNum][colNum]+"--");
		 //System.out.println();
	 }
	 
 } 
}
