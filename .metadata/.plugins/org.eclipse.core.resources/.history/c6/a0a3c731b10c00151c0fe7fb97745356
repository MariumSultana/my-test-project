
public class New1 {
 public static void main(String[] args) {

	 int i[][]=new int[3][3];
	 i[0][0]=2;
	 i[0][1]=3;
	 i[0][2]=4;
	 
	 i[1][0]=2;
	 i[1][1]=3;
	 i[1][2]=4;
	 
	 i[2][0]=2;
	 i[2][1]=3;
	 i[2][2]=4;
	 for(int a=0; a<i.length; a++){
		 System.out.println(i[a]);
		 System.out.println(i.length);
	 }
	 
 } 
}
